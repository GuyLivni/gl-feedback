{
  "name": "feedback-client",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/auth/*": {
      "target": "http://localhost:5000"
    },
    "/api/*": {
      "target": "http://localhost:5000"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --cache",
      "git add",
      "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests"
    ]
  },
  "pre-commit": "precommit",
  "dependencies": {
    "axios": "^0.18.0",
    "deline": "^1.0.4",
    "invariant": "^2.2.3",
    "moment": "^2.19.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^3.1.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-logger": "^3.0.6",
    "semantic-ui-css": "2.2.12",
    "semantic-ui-react": "0.78.2",
    "styled-components": "^3.1.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./ --fix --cache",
    "flow": "flow check",
    "precommit": "flow check && lint-staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.3.13",
    "@storybook/addon-actions": "^3.3.13",
    "@storybook/addon-info": "^3.3.13",
    "@storybook/addon-knobs": "^3.3.13",
    "@storybook/addon-links": "^3.3.13",
    "@storybook/addon-options": "^3.3.13",
    "@storybook/addons": "^3.3.13",
    "@storybook/react": "^3.3.13",
    "babel-core": "^6.26.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.66.0",
    "jest-styled-components": "^5.0.0",
    "lint-staged": "^7.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.10.2",
    "react-test-renderer": "^16.2.0"
  }
}
